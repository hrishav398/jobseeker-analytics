{
  "name": "Job Tracker Dev",
  "features": {
    "docker-in-docker": "latest"
  },
  "forwardPorts": [3000, 8000, 5433],
  "postCreateCommand": "docker compose -f docker-compose-dev.yaml up -d",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true,
        "files.autoSave": "onFocusChange"
      },
      "extensions": [
        "ms-azuretools.vscode-docker",
        "ms-python.python",
        "msjsdiag.vscode-react-native"
      ]
    }
  }
}
